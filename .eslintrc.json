{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "react-native",
      "jsx-a11y",
      "import"
    ],
    "rules": {
      "react/jsx-filename-extension": 0,
      "react/prefer-stateless-function": 1,
      "react/jsx-no-bind": 0,
      "react/prop-types": 0,
      "no-underscore-dangle": 0,
      "no-restricted-syntax": 0,
      "consistent-return": 0,
      "max-len": 1,
      "arrow-body-style": 1
    },
    "globals": {
      "fetch": false
    }
}
